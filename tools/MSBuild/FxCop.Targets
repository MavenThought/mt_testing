<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="FxCop">
    <MakeDir Directories="Output"/>
    <MSBuild.ExtensionPack.CodeQuality.FxCop TaskAction="Analyse" FxCopPath="tools\fxcop\fxcopcmd.exe"
                                             Project="Price.Displacement.Desktop.sln.fxcop"
                                             ReportXsl="tools\fxcop\Xml\FxCopReport.xsl"
                                             OutputFile="output\fxcop.html">
